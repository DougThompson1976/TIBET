{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "TP": true,
        "APP": true,
        "Window": false,
        "self": false
    },
    "rules": {
        "camelcase": 0,
        "consistent-return": 0,
        "one-var": 1,
        "no-extend-native": 0,
        "no-floating-decimal": 1,
        "no-shadow": 0,
        "no-unused-expressions": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "no-wrap-func": 1,
        "quotes": [2, "single"],
        "radix": 1,
        "space-after-keywords": [1, "always"],
        "space-before-blocks": [1, "always"],
        "space-in-brackets": [1, "never"],
        "strict": 0,
        "wrap-iife": [1, "outside"],
        "vars-on-top": 1
    }
}
