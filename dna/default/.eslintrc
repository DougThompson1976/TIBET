{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "TP": true,
        "APP": true,
        "Window": false,
        "self": false
    },
    "rules": {
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 0,
        "consistent-return": 0,
        "consistent-this": [0, "thisref"],
        "default-case": 0,
        "eol-last": 0,
        "guard-for-in": 2,
        "max-len": [0, 80, 4],
        "no-div-regex": 2,
        "no-eq-null": 0,
        "no-extend-native": 0,
        "no-extra-parens": 1,
        "no-floating-decimal": 1,
        "no-multiple-empty-lines": 1,
        "no-nested-ternary": 1,
        "no-reserved-keys": 1,
        "no-self-compare": 2,
        "no-shadow": 0,
        "no-unused-expressions": 0,
        "no-unused-vars": [0, {"vars": "all", "args": "none"}],
        "no-underscore-dangle": 0,
        "no-wrap-func": 1,
        "one-var": 1,
        "quotes": [2, "single"],
        "quote-props": [2, "as-needed"],
        "radix": 1,
        "space-after-function-name": [2, "never"],
        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],
        "space-in-brackets": [2, "never"],
        "space-in-parens": [2, "never"],
        "spaced-line-comment": [0, "always"],
        "strict": 0,
        "wrap-iife": [1, "outside"],
        "vars-on-top": 1
    }
}
